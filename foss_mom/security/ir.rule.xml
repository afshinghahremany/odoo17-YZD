<?xml version="1.0" encoding="utf-8"?>

<odoo>
	<data noupdate="1">

    <!-- Record Rules -->

        <record id="calendar_event_rule" model="ir.rule">
            <field name="name">Calendar event</field>
            <field name="model_id" ref="foss_mom.model_calendar_event"/>
            <field name="perm_read" eval="1"/>
